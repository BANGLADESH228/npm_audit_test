    "buffer": "^6.0.3",
    "google-protobuf": "^3.21.2",
    "html-react-parser": "^0.9.1",
    "lerna": "^3.19.0",
    "octokit": "^4.0.2",
    "os-browserify": "^0.3.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "14.3.8",
    "react-file-drop": "^0.2.8",
    "react-ga": "^2.6.0",
    "react-scripts": "5.0.1",
    "react-star-rating-component": "^1.4.1",
